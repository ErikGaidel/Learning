      "source": [
        "text, n = input(), int(input())\n",
        "for _ in range(n):\n",
        "    i, j, k = map(int,input().split())\n",
        "    cut = text[i:j+1]\n",
        "    text = text[:i] + text[j+1:]\n",
        "    if k:\n",
        "        text = text[:k] + cut + text[k:]\n",
        "    else:\n",
        "        text = cut + text\n",
        "print(text)"
      ]
    }
  ]
